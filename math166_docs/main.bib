@article{berg1977,
	abstract = {Statistical fluctuations limit the precision with which a microorganism can, in a given time T, determine the concentration of a chemoattractant in the surrounding medium. The best a cell can do is to monitor continually the state of occupation of receptors distributed over its surface. For nearly optimum performance only a small fraction of the surface need be specifically adsorbing. The probability that a molecule that has collided with the cell will find a receptor is Ns/(Ns + pi a), if N receptors, each with a binding site of radius s, are evenly distributed over a cell of radius a. There is ample room for many indenpendent systems of specific receptors. The adsorption rate for molecules of moderate size cannot be significantly enhanced by motion of the cell or by stirring of the medium by the cell. The least fractional error attainable in the determination of a concentration c is approximately (TcaD) - 1/2, where D is diffusion constant of the attractant. The number of specific receptors needed to attain such precision is about a/s. Data on bacteriophage absorption, bacterial chemotaxis, and chemotaxis in a cellular slime mold are evaluated. The chemotactic sensitivity of Escherichia coli approaches that of the cell of optimum design.},
	author = {Berg, H.C. and Purcell, E.M.},
	date-added = {2018-04-18 19:18:31 +0000},
	date-modified = {2018-04-18 19:18:43 +0000},
	doi = {https://doi.org/10.1016/S0006-3495(77)85544-6},
	issn = {0006-3495},
	journal = {Biophysical Journal},
	number = {2},
	pages = {193 - 219},
	title = {Physics of chemoreception},
	url = {http://www.sciencedirect.com/science/article/pii/S0006349577855446},
	volume = {20},
	year = {1977},
	bdsk-url-1 = {http://www.sciencedirect.com/science/article/pii/S0006349577855446},
	bdsk-url-2 = {https://doi.org/10.1016/S0006-3495(77)85544-6}}

@article{aquino2016,
	abstract = {How cells reliably infer information about their environment is a fundamentally important question. While sensing and signaling generally start with cell-surface receptors, the degree of accuracy with which a cell can measure external ligand concentration with even the simplest device---a single receptor---is surprisingly hard to pin down. Recent studies provide conflicting results for the fundamental physical limits. Comparison is made difficult as different studies either suggest different readout mechanisms of the ligand-receptor occupancy, or differ on how ligand diffusion is implemented. Here we critically analyse these studies and present a unifying perspective on the limits of sensing, with wide-ranging biological implications.},
	author = {Aquino, Gerardo and Wingreen, Ned S. and Endres, Robert G.},
	date-added = {2018-04-18 00:07:53 +0000},
	date-modified = {2018-04-18 19:18:04 +0000},
	day = {01},
	doi = {10.1007/s10955-015-1412-9},
	issn = {1572-9613},
	journal = {Journal of Statistical Physics},
	keywords = {cell sensing, information theory, signalling},
	month = {Mar},
	number = {5},
	pages = {1353--1364},
	title = {Know the Single-Receptor Sensing Limit? Think Again},
