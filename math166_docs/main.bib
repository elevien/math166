	journal = {Journal of Computational Physics},
	keywords = {Chemical reaction networks, Monte Carlo, Variance reduction, Piecewise deterministic Markov process, Gillespie algorithm},
	pages = {1 - 13},
	title = {Coupling sample paths to the thermodynamic limit in Monte Carlo estimators with applications to gene expression},
